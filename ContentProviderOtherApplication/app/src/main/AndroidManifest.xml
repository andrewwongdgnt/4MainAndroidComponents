<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fourmainandroidcomponents.contentproviderotherapplication">
    <permission android:name="com.fourmainandroidcomponents.contentproviderotherapplication.READ_PERMISSION" android:protectionLevel="normal"  />
    <permission android:name="com.fourmainandroidcomponents.contentproviderotherapplication.WRITE_PERMISSION" android:protectionLevel="normal"  />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ContentProviderOtherApplication">
        <provider
            android:name=".MyContentProvider"
            android:authorities="com.fourmainandroidcomponents.contentproviderotherapplication"
            android:enabled="true"
            android:exported="true"
            android:readPermission="com.fourmainandroidcomponents.contentproviderotherapplication.READ_PERMISSION"
            android:writePermission="com.fourmainandroidcomponents.contentproviderotherapplication.WRITE_PERMISSION"/>
    </application>

</manifest>